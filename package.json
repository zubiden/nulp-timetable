{
  "name": "nulp-timetable",
  "version": "1.2.1",
  "description": "NULP timetable",
  "main": "''",
  "engines": {
    "node": "^16.0",
    "npm": "^8.1"
  },
  "scripts": {
    "clean": "shx rm -rf build",
    "webpack": "webpack --config config/webpack.config.production.js",
    "build": "npm run clean && npm run webpack",
    "lint": "eslint src config",
    "dev": "webpack serve --mode development --config config/webpack.config.dev.js",
    "start": "serve -s build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zubiden/nulp-timetable"
  },
  "author": "zubiden",
  "license": "MIT",
  "homepage": "https://zubiden.github.io/nulp-timetable",
  "dependencies": {
    "css-minimizer-webpack-plugin": "^3.4.1",
    "history": "^5.2.0",
    "idb": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-search-panel": "^1.0.9"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "del-cli": "^4.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.8.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "semantic-release": "^19.0.2",
    "serve": "^13.0.2",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^4.7.4",
    "workbox-webpack-plugin": "^6.4.2"
  }
